{
  "name": "execution-graph-gui",
  "productName": "ExecutionGraphGui",
  "version": "1.0.0",
  "description": "The execution graph native gui application based on electron.",
  "repository": "https://github.com/gabyx/ExecutionGraph",
  "author": "GitHub",
  "license": "MPL 2.0",
  "main": "main.js",
  "files": [
    "./src/main.js"
  ],
  "scripts": {
    "build": "tsc && cp -r resources dist && cp package.json.template dist/package.json",
    "build-start": "npm run build && electron ./dist/main.js",
    "build-and-package": "npm run build && electron-packager ./dist --out build",
    "start": "electron ./dist/main.js"
  },
  "devDependencies": {
    "electron": "^3.0.8",
    "electron-packager": "^12.2.0",
    "typescript": "3.1.3"
  },
  "dependencies": {
    "argparse": "^1.0.10"
  }
}
