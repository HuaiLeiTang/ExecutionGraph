struct Vector2f
{
    x:float32;
    y:float32;
}

struct Vector2d
{
    x:float64;
    y:float64;
}

struct Vector3d
{
    x:float64;
    y:float64;
    z:float64;
}

struct Box2f
{
    positionMin:Vector2f (id:0);
    positionMax:Vector2f (id:1);
}

